{
  "description": "Test all util without individual `package.json`s",
  "name": "util",
  "type": "module",
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "build:js": "babel src -d dist -x .ts --plugins=add-import-extension --ignore **/__tests__/**,**/*.d.ts --source-maps",
    "build:ts": "tsc",
    "build": "npm run build:js & npm run build:ts"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/preset-typescript": "^7.18.6",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "babel-plugin-add-import-extension": "^1.6.0",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "tslib": "^2.4.1"
  }
}